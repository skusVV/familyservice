{
  "name": "familyservice",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:prod": "ng run familyservice:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod",
    "build:prod": "npm run build:server:prod && npm run build:browser:prod",
    "artifact": "rimraf ./tmp/build.tar.gz && mkdir -p tmp && cd ./dist && tar -zcvf ./../tmp/build.tar.gz * && cd ../",
    "gzip": "cd ./dist/browser && gzip main.*.js && gzip styles.*.css && gzip polyfills.*.js ",
    "server": "node local.js",
    "proxy": "ng serve --proxy-config proxy.conf.json"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/pwa": "^0.8.5",
    "@angular/router": "^6.1.0",
    "@angular/service-worker": "^6.1.0",
    "@ng-toolkit/universal": "^1.1.41-beta.110",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "ngx-infinite-scroll": "^6.0.1",
    "rxjs": "~6.2.0",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "4.2.0",
    "uuid": "^3.3.2",
    "webpack-cli": "^3.1.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
